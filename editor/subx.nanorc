# include this file in your ~/.nanorc

syntax "subx" "\.subx$"
# function definitions
color yellow "^[a-z][^ ]*:"
# global variables
color red "^[A-Z][^ ]*:"
# tests
color green "^test-[^ ]*:"
# strings literals
color cyan ""([^"\]|\\.)+""
# comments
comment "#"
color brightblue "# - .*"
color blue "#.*$"
color brightblack "# \. .*"
