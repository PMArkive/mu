{
    "scopeName": "source.subx",
    "name": "SubX",
    "fileTypes": ["subx"],
    "patterns": [
        {
            "name": "header-comment.subx",
            "match": "# - .*"
        },
        {
            "name": "comment-sub-2.subx",
            "match": "# \\. \\. .*"
        },
        {
            "name": "comment-sub-1.subx",
            "match": "# \\. .*"
        },
        {
            "name": "comment.subx",
            "match": "#.*"
        },
        {
            "name": "strings.subx",
            "match": "\"[^\"]*\""
        },
        {
            "name": "test.subx",
            "match": "^test-[^ ]*:"
        },
        {
            "name": "global.subx",
            "match": "^[A-Z][^ ]*:"
        },
        {
            "name": "function.subx",
            "match": "^[^_$ #][^ ]*:"
        },
        {
            "name": "label.subx",
            "match": "^[_$][^ ]*:"
        }
    ]
}
